# 애플리케이션 기본 설정
APP_NAME=Llama-PcapLog Analysis API
APP_VERSION=1.0.0
DEBUG=false

# 서버 설정
HOST=0.0.0.0
PORT=8000

# CORS 설정
CORS_ORIGINS=["*"]
CORS_CREDENTIALS=true
CORS_METHODS=["*"]
CORS_HEADERS=["*"]

# 파일 업로드 설정
UPLOAD_DIR=uploads

# 모델 설정
DEFAULT_MODEL=LLaMa-PcapLog
MODEL_CACHE_DIR=

# 로깅 설정
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s 